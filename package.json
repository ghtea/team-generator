{
  "name": "TeamGenerator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "a-d": "react-native run-android --verbose --appIdSuffix=debug",
    "a-c": "cd ./android && ./gradlew --stop && ./gradlew cleanBuildCache",
    "i-d": "react-native run-ios --verbose --configuration=Debug",
    "i-c": "rm -rf ./ios/build && rm -rf ./ios/Pods && rm -rf ./ios/Podfile.lock && cd ./ios && arch -x86_64 pod install && cd .."
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "react": "17.0.2",
    "react-native": "0.64.1",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/react-native": "^0.64.13",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
